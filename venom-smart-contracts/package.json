{
  "name": "dex",
  "version": "1.0.0",
  "description": "DEX",
  "main": "index.js",
  "scripts": {
    "build": "npx locklift build --config locklift.config.ts",
    "migration-prod": "locklift run --config locklift.config.js --network main --script scripts/migration-prod.js && locklift test --config locklift.config.js --network main --test test/prod",
    "test": "sh test.sh",
    "test:oracle-upgrade": "npx locklift test --config locklift.config.js --disable-build --network local --tests test/upgrade/5-oracle-upgrade-test.js",
    "deploy:account": "npx locklift run --disable-build --script v2/scripts/0-deploy-account.ts --config locklift.config.ts --network test",
    "deploy:dex": "npx locklift run --disable-build --script v2/scripts/prod/deploy.ts --config locklift.config.ts --network",
    "deploy:tokens": "npx locklift run --disable-build --script v2/scripts/2-deploy-test-tokens.ts --config locklift.config.ts --network",
    "deploy:pair": "npx locklift run --disable-build --script v2/scripts/5-deploy-test-pair.ts --config locklift.config.ts --network",
    "deploy:dex-account": "npx locklift run --disable-build --script v2/scripts/4-deploy-test-dex-account.ts --config locklift.config.ts --network",
    "export:dex-accounts": "npx locklift run --disable-build --script v2/scripts/prod/export-dex-accounts.ts --config locklift.config.ts --network",
    "export:dex-pairs": "npx locklift run --disable-build --script v2/scripts/prod/export-dex-pairs.ts --config locklift.config.ts --network",
    "upgrade:dex-accounts": "npx locklift run --disable-build --script v2/scripts/prod/force-upgrade-dex-accounts-batch.ts --config locklift.config.ts --network",
    "upgrade:dex-pairs": "npx locklift run --disable-build --script v2/scripts/prod/upgrade-dex-pairs-batch.ts --config locklift.config.ts --network",
    "active:dex-pairs": "npx locklift run --disable-build --script v2/scripts/prod/set-pairs-active-batch.ts --config locklift.config.ts --network"
  },
  "author": "aleksandr-hramcov <aleksandr.hramcov@gmail.com>",
  "license": "AGPL-3.0",
